setenv image_name Image.gz
setenv fdt_name armada-8040-mcbin.dtb
setenv bootmmc 'mmc dev 1; ext4load mmc 1:2 $kernel_addr $image_name;ext4load mmc 1:2 $fdt_addr $fdt_name;setenv bootargs $console root=/dev/mmcblk1p1 rw rootwait; booti $kernel_addr - $fdt_addr'

run bootmmc

printenv


mmc dev 1
ext4load mmc 1:2 $kernel_addr Image
ext4load mmc 1:2 $fdt_addr armada-8040-mcbin.dtb
booti $kernel_addr - $fdt_addr


